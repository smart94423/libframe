{
  "scripts": {
    "// Release new version": "",
    "release": "ts-node ./release.ts",
    "// Install all examples and boilerplates": "",
    "install:examples": "ts-node ./install-examples.ts",
    "// Linkage": "",
    "link-src": "ts-node ./link-src.ts",
    "unlink-src": "ts-node ./unlink-src.ts",
    "link-vite": "ts-node ./link-vite.ts",
    "unlink-vite": "ts-node ./unlink-vite.ts",
    "// Update all dependencies": "",
    "bump": "yarn bump:dependencies && yarn bump:lockfiles && yarn bump:commit && yarn bump:test",
    "bump:dependencies": "ts-node ./bump-dependencies.ts",
    "bump:lockfiles": "ts-node ./bump-lockfiles.ts",
    "bump:commit": "cd ../../ && git add . && git commit -am 'chore: update dependencies' &2> /dev/null",
    "bump:test": "cd ../../ && yarn test"
  },
  "dependencies": {
    "@types/node": "^15.6.1",
    "@types/semver": "^7.3.6",
    "conventional-changelog-cli": "^2.1.1",
    "execa": "^5.0.0",
    "kolorist": "^1.4.1",
    "npm-check-updates": "^11.5.13",
    "semver": "^7.3.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "yarn": "^1.22.10"
  },
  "// Needed for Yarn workspaces": "",
  "name": "scripts",
  "version": "0.0.1"
}
